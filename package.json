{
  "name": "ludoking",
  "version": "1.0.0",
  "description": "Multiplayer Ludo Game",
  "main": "server.js",
  "engines": {
    "node": "12.14.1",
    "npm": "6.14.4"
  },
  "scripts": {
    "client-install": "cd client && npm install",
    "start": "node server.js",
    "server": "nodemon server.js",
    "client": "npm start --prefix client",
    "dev": "concurrently \"npm run server\" \"npm run client\""
  },
  "author": "Deepak M",
  "license": "",
  "devDependencies": {
    "nodemon": "^1.14.6"
  },
  "dependencies": {
    "concurrently": "^3.5.1",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "socket.io": "^2.3.0"
  }
}
